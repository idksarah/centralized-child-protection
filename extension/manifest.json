{
  "manifest_version": 3,
  "name": "Centralized Child Protection",
  "version": "0.0",
  "description": "Protects your children",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage", "activeTab", "scripting"]
}
